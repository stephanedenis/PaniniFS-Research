{
  "name": "paninifs-workspace",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Workspace Node pour les sous-projets techniques PaniniFS (tests, outils).",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "test": "npm --workspace @paninifs/test-runner run test",
    "test:firefox": "npm --workspace @paninifs/test-runner run test:firefox",
    "test:headed": "npm --workspace @paninifs/test-runner run test:headed",
    "test:ui": "npm --workspace @paninifs/test-runner run test:ui",
    "test:report": "npm --workspace @paninifs/test-runner run test:report",
    "codec:test": "npm --workspace @paninifs/semantic-codec run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stephanedenis/PaniniFS-Research.git"
  },
  "license": "ISC",
  "dependencies": {
    "node-fetch": "^3.3.2",
    "puppeteer": "^24.22.0"
  }
}
